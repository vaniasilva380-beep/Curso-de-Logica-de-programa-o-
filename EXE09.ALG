algoritmo "exe09"
var
   volume , raio :real

inicio

   //temos uma constante pi 3.14159
   //entrada
   escreva("valor do raio:")
   leia (raio)



   //ptocessamento

   volume <- 4/3.*3.14159 * raio ^ 3


   //saida

   escreval ("volume:=" ,volume:8:3 )




fimalgoritmo